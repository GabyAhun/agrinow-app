        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }
    var desiredDate = g_form.getValue('desired_delivery_date');
    var lastDate = g_form.getValue('last_accepted_delivery_date');

    if (desiredDate >= lastDate) {
		alert('The last accepted date of delivery cannot be less than the desired date!');
		g_form.clearValue('last_accepted_delivery_date');
		return;
    }
}]]></script>
