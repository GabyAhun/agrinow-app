        <description>This script is going to set the ID field based on the Category.</description>
        <field>category</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Automatic ID from Category</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    var animalCategory = g_form.getValue('category');
	
	var animalNumber = g_form.getValue('number');
	var categoryArray = animalNumber.split('');
	var lastFiveChars = categoryArray.slice(-5);
	var resultString = lastFiveChars.join('');

	g_form.setValue('id', resultString);

    //Type appropriate comment here, and begin script below

}]]></script>
